#!/bin/sh
#
# This script will plot ray paths using GMT. If you want to
#use this as a data file for psxy in another script, delete these
# first lines, to the last psxy, as well as the last line.
#
/bin/rm -f taup_path.ps
# draw surface and label distances.
gmt psbasemap -K -P -R0/360/0/6371.0 -JP6.0i -Bx30  > taup_path.ps
# draw circles for branches, note these are scaled for a 
# map using -JP6.0i
gmt psxy -K -O -P -R -JP -Sc -A >> taup_path.ps <<ENDLAYERS
0.0 0.0 6.0i
0.0 0.0 6.0i
0.0 0.0 5.9811645i
0.0 0.0 5.967038i
0.0 0.0 5.802229i
0.0 0.0 5.6138754i
0.0 0.0 5.3784337i
0.0 0.0 3.279234i
0.0 0.0 1.1462251i
ENDLAYERS

# draw paths
gmt psxy -P -R -K -O -JP -m -A >> taup_path.ps <<END
